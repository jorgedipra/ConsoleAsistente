html,
body {
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  font-family: 'Roboto', sans-serif;
  font-size: 14px;
  background-color: #eeeeee;
  color: #949494;
}

* {
  box-sizing: border-box; 
}
li{
  list-style: none;
}
a {
  text-decoration: none;
}
code{
  color: #19d264
  width 300px;
  height 300px
  position absolute
  background: #000
  border: solid 1px #000
  font-size: 25px
}
#actividad{
  position absolute
  z-index 1
  margin: 280px 300px
  width 200px;
}
#historial{
  margin: 80px 300px
  width 200px;
  height 200px
  position absolute
  background: #ffffff
  border: solid 1px #000
  overflow: auto
  #msginicial{
    height: 200px;
    display: -webkit-box;
  }
}
.Alis{
  color #000
  margin-left: -5px
  width 100%
  .message{
    background: rgba(247, 11, 117, 0.369)
    padding: 2px 5px
    border-radius: 5px
    position: absolute;
  }
}
.User{
  color #000 
  margin-left -30px
  width 100%
  .message{
    background: #2c90395e
    padding: 2px 5px
    border-radius: 5px
    position: absolute;
  }
}